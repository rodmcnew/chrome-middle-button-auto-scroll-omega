{
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "index.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*",
        "ftp://*/*"
      ],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "128": "icon128.png"
  },
  "description": "Adds middle mouse button auto-scroll support to chrome.",
  "manifest_version": 2,
  "minimum_chrome_version": "55.0.0.0",
  "name": "Middle Button Auto Scroll Omega",
  "version": "0.0.1"
}
